{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "rgName": {
      "value": "ent-dev-fhir-rg"
    },
    "availabilityZones": {
      "value": [
        "1",
        "2",
        "3"
      ]
    },
    "vnetSpokeName": {
      "value": "ent-dev-fhir-vnet"
    },
    "spokeVNETaddPrefixes": {
      "value": [
        "10.0.0.0/16",
        "10.1.0.0/16"
      ]
    },
    "spokeSubnets": {
      "value": [
        {
          "name": "default",
          "properties": {
            "addressPrefix": "10.0.0.0/24"
          }
        },
        {
          "name": "AzureFirewallSubnet",
          "properties": {
            "addressPrefix": "10.0.1.0/26"
          }
        },
        {
          "name": "ent-dev-fhir-subnet",
          "properties": {
            "addressPrefix": "10.1.1.0/24"
          }
        },
        {
          "name": "ent-dev-fhir-appgw-subnet",
          "properties": {
            "addressPrefix": "10.1.2.0/27",
            "serviceEndpoints": [
              {
                "service": "Microsoft.Storage"
              }
            ]
          }
        },
        {
          "name": "ent-dev-fhir-private-endpoint-subnet",
          "properties": {
            "addressPrefix": "10.1.4.0/24",
            "privateEndpointNetworkPolicies": "Disabled"
          }
        },
        {
          "name": "ent-dev-fhir-apim-subnet",
          "properties": {
            "addressPrefix": "10.1.5.0/24"
          }
        }
      ]
    },
    "azfwName": {
      "value": "ent-dev-fhir-fw"
    },
    "fwapplicationRuleCollections": {
      "value": [
        {
          "name": "ent-dev-fhir-fw-app-rule-helper-tools",
          "properties": {
            "priority": 101,
            "action": {
              "type": "Allow"
            },
            "rules": [
              {
                "name": "Allow-ifconfig",
                "protocols": [
                  {
                    "port": 80,
                    "protocolType": "Http"
                  },
                  {
                    "port": 443,
                    "protocolType": "Https"
                  }
                ],
                "targetFqdns": [
                  "ifconfig.co",
                  "jsonip.com",
                  "motd.ubuntu.com"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ]
              }
            ]
          }
        },
        {
          "name": "ent-dev-fhir-fw-app-rule-egress-application",
          "properties": {
            "priority": 102,
            "action": {
              "type": "Allow"
            },
            "rules": [
              {
                "name": "Egress",
                "protocols": [
                  {
                    "port": 443,
                    "protocolType": "Https"
                  }
                ],
                "targetFqdns": [
                  "*.blob.core.windows.net",
                  "*.cdn.mscr.io",
                  "*.opinsights.azure.com",
                  "*.monitoring.azure.com"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ]
              },
              {
                "name": "Registries",
                "protocols": [
                  {
                    "port": 443,
                    "protocolType": "Https"
                  }
                ],
                "targetFqdns": [
                  "*.azurecr.io",
                  "*.gcr.io",
                  "*.docker.io"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ]
              },
              {
                "name": "Additional-Usefull-Address",
                "protocols": [
                  {
                    "port": 443,
                    "protocolType": "Https"
                  }
                ],
                "targetFqdns": [
                  "grafana.net",
                  "grafana.com",
                  "stats.grafana.org",
                  "github.com",
                  "raw.githubusercontent.com",
                  "*.letsencrypt.org",
                  "vortex.data.microsoft.com"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ]
              }
            ]
          }
        }
      ]
    },
    "fwnetworkRuleCollections": {
      "value": [
        {
          "name": "ent-dev-fhir-fw-net-rule-net-egress",
          "properties": {
            "priority": 200,
            "action": {
              "type": "Allow"
            },
            "rules": [
              {
                "name": "NTP",
                "protocols": [
                  "UDP"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ],
                "destinationAddresses": [
                  "*"
                ],
                "destinationPorts": [
                  "123"
                ]
              },
              {
                "name": "APITCP",
                "protocols": [
                  "TCP"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ],
                "destinationAddresses": [
                  "*"
                ],
                "destinationPorts": [
                  "9000"
                ]
              },
              {
                "name": "APIUDP",
                "protocols": [
                  "UDP"
                ],
                "sourceAddresses": [
                  "10.1.1.0/24"
                ],
                "destinationAddresses": [
                  "*"
                ],
                "destinationPorts": [
                  "1194"
                ]
              }
            ]
          }
        }
      ]
    },
    "fwnatRuleCollections": {
      "value": []
    },
    "dhcpOptions": {
      "value": {
        "dnsServers": [
          "10.0.1.4"
        ]
      }
    },
    "appGatewaySubnetName":{
      "value": "ent-dev-fhir-appgw-subnet"
    },
    "rtFHIRSubnetName": {
      "value": "ent-dev-fhir-subnet-rt"
    },
    "firewallIP": {
      "value": "10.0.1.4"
    },
    "rtAppGWSubnetName": {
      "value": "ent-dev-fhir-appgw-subnet-rt"
    },
    "nsgFHIRName": {
      "value": "ent-dev-fhir-nsg"
    },
    "FHIRSubnetName": {
      "value": "ent-dev-fhir-subnet"
    },
    "nsgAppGWName": {
      "value": "ent-dev-fhir-appgw-nsg"
    },
    "resourceSuffix": {
      "value": "ahds"
    }
  }
}
